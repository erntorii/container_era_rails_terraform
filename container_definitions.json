[
  {
    "name": "rails_webapp",
    "image": "650711581682.dkr.ecr.ap-northeast-1.amazonaws.com/rails_webapp:latest",
    "cpu": 333,
    "memoryReservation": 600,
    "essential": true,
    "portMappings": [
      {
        "hostPort": 0,
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "command": ["bundle", "exec", "pumactl", "start"],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "rails-service",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "service"
      }
    },
    "environment": [
      {
        "name": "RAILS_ENV",
        "value": "production"
      },
      {
        "name": "RACK_ENV",
        "value": "production"
      }
    ]
  }
]
